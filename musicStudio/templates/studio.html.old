let mediaRecorder;
let recordedChunks = [];

const audioSourceSelect = document.getElementById('audioSource');
const startBtn = document.getElementById('startBtn');
const stopBtn = document.getElementById('stopBtn');
const audioPlayback = document.getElementById('audioPlayback');

startBtn.addEventListener('click', startRecording);
stopBtn.addEventListener('click', stopRecording);

async function startRecording() {
    const audioSource = audioSourceSelect.value;
    let stream;

    if (audioSource === 'microphone') {
        stream = await navigator.mediaDevices.getUserMedia({ audio: true });
    } else if (audioSource === 'system') {
        // For system audio, you might need additional permissions or browser extensions
        // The below approach is for illustration; it won't work without additional setup
        stream = await navigator.mediaDevices.getDisplayMedia({ audio: true, video: true });
    } else {
        alert('Unsupported audio source selected');
        return;
    }

    mediaRecorder = new MediaRecorder(stream);
    mediaRecorder.ondataavailable = handleDataAvailable;
    mediaRecorder.start();

    startBtn.disabled = true;
    stopBtn.disabled = false;
}

function stopRecording() {
    mediaRecorder.stop();
    startBtn.disabled = false;
    stopBtn.disabled = true;
}

function handleDataAvailable(event) {
    if (event.data.size > 0) {
        recordedChunks.push(event.data);
        const blob = new Blob(recordedChunks, { type: 'audio/webm' });
        const url = URL.createObjectURL(blob);
        audioPlayback.src = url;
        recordedChunks = [];
    }
}

